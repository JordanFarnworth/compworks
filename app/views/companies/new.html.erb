<div class="container">
  <div class="row center">
    <div class="col-md-6 col-md-offset-3">
      <h4><a href="/admin_dashboard"><i class="fa fa-sign-out" aria-hidden="true"></i></a>  Add New Company</h4>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= bootstrap_form_for(@company) do |f| %>
        <%= f.text_field :name %>
        <%= f.text_field :network %>
        <%= f.text_field :domain %>
        <%= f.text_field :antivirus %>
        <%= f.text_field :router1 %>
        <%= f.text_field :router2 %>
        <%= f.text_field :doctor_name %>
        <%= f.submit "Create Company" %>
      <% end %>
    </div>
  </div>
</div>
