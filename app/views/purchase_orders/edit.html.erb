<div class="container-fluid">
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center" id="po-number-edit">

      </div>
    </div>
    <div class="row" id="loading-space">
      <div class="col-md-6 col-md-offset-3">
        <form>
          <div class="checkbox">
             <label>
               <input type="checkbox" id="received-input-edit"> Received
             </label>
           </div>
          <fieldset class="form-group">
            <label for="vendor-input">Vendor</label>
            <input class="form-control" id="vendor-input-edit" placeholder="Start typing for vendor..">
            <small class="text-muted">If the vendor does not exist, a new one will be created.</small>
          </fieldset>
          <fieldset class="form-group">
            <label for="client-input">Client</label>
            <input class="form-control" id="client-input-edit" placeholder="Search for client">
            <small class="text-muted">This should autocomplete for you</small>
          </fieldset>
          <fieldset class="form-group">
            <label for="item-input">Items</label>
            <input class="form-control" id="item-input-edit" placeholder="Search for Item..">
            <small class="text-muted">This should autocomplete for you. If the item doesn't exist, it will be saved for next time you create a PO.</small>
          </fieldset>
          <fieldset class="form-group">
           <label for="InputFile">PO Image</label>
           <input type="file" class="form-control-file" id="InputFile">
         </fieldset>
         <fieldset>
           <div id="current-image">
             <label >Current Image</label>
             <br>

           </div>
         </fieldset>
         <fieldset>
           <br>
           <button class="btn btn-default" type="button" id="po-update">Update PO</button>
         </fieldset>
        </form>
      </div>
    </div>
  </div>
</div>
